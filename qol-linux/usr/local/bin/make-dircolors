*.tz=$ARC
*.deb=$ARC
*.rpm=$ARC
*.jar=$ARC
*.war=$ARC
*.ear=$ARC
*.sar=$ARC
*.rar=$ARC
*.alz=$ARC
*.ace=$ARC
*.zoo=$ARC
*.cpio=$ARC
*.7z=$ARC
*.rz=$ARC
*.cab=$ARC
*.jpg=$PHOTO
*.jpeg=$PHOTO
*.gif=$PHOTO
*.bmp=$PHOTO
*.pbm=$PHOTO
